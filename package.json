  "keywords": [
    "gatsby",
    "mdx",
    "typescript",
    "prismaio",
    "howtodata",
    "databases",
    "styled-components"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "clean": "gatsby clean",
    "build": "npm run install-algolia && gatsby build --prefix-paths --log-pages",
    "dev": "gatsby develop",
    "serve": "npm run build && gatsby serve --prefix-paths -o",
    "format": "prettier . --write --check",
    "test": "jest",
    "install-algolia": "cd ./plugins/gatsby-algolia-indexer && npm install",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@mdx-js/loader": "2.1.2",
    "@mdx-js/tag": "0.20.3",
    "@parcel/namer-default": "^2.5.0",
    "@types/jest": "28.1.4",
    "@types/react": "18.0.15",
    "@types/react-helmet": "6.1.5",
    "@types/react-sticky-el": "1.0.3",
    "@types/styled-components": "5.1.25",
    "gatsby-remark-sectionize": "1.0.0",
    "husky": "8.0.1",
    "jest": "28.1.2",
    "jest-dom": "4.0.0",
    "lint-staged": "13.0.3",
    "loader-utils": "3.2.0",
    "prettier": "2.7.1",
    "prettier-plugin-prisma": "4.1.0",
    "prisma": "4.0.0",
    "ts-jest": "28.0.5",
    "tslint": "5.20.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.*": "prettier --write --ignore-unknown"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "percy": {
    "version": 1,
    "snapshot": {
      "enable-javascript": true,
      "widths": [
        1280
      ]
    }
  }
}
